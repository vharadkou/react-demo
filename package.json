{
  "name": "react-mobx",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.0.1",
    "notistack": "^0.4.2",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "npm run lint-ts && export NODE_PATH=./src && react-scripts start",
    "build": "npm run lint-ts && export NODE_PATH=./src && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c tslint.json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/uuid": "^3.4.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "react-scripts": "^2.1.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
